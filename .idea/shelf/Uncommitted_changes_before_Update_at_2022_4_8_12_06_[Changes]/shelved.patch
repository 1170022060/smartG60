<+>package com.pingok.vocational.mapper.device;\r\n\r\nimport com.pingok.vocational.domain.device.TblDeviceInfo;\r\nimport com.pingok.vocational.domain.device.TblDeviceInfoGantry;\r\nimport com.ruoyi.common.core.mapper.CommonRepository;\r\nimport org.apache.ibatis.annotations.Param;\r\nimport org.apache.ibatis.annotations.Select;\r\n\r\nimport java.util.List;\r\nimport java.util.Map;\r\n\r\n/**\r\n * TBL_DEVICE_INFO_GANTRY 数据层\r\n *\r\n * @author xia\r\n */\r\n\r\npublic interface TblDeviceInfoGantryMapper extends CommonRepository<TblDeviceInfoGantry> {\r\n\r\n    /**\r\n     * 通过设备类目、设备状态、安装地点、设备ID、使用方、管理方、使用寿命查询门架信息\r\n     * @param deviceCategory 设备类目\r\n     * @param status 设备状态\r\n     * @param fieldBelong 安装地点\r\n     * @param deviceId 设备ID\r\n     * @param userSide 使用方\r\n     * @param managementSide 管理方\r\n     * @param serviceLife 使用寿命\r\n     * @return 设备对应信息\r\n     */\r\n    @Select({\"<script>\" +\r\n            \"select a.ITEM_NAME as \\\"itemName\\\" ,\" +\r\n            \"b.CATEGORY_NAME as \\\"deviceCategory\\\" ,\" +\r\n            \"a.DEVICE_ID as \\\"deviceId\\\" ,\" +\r\n            \"a.DEVICE_NAME as \\\"deviceName\\\" ,\" +\r\n            \"a.DEVICE_BRAND as \\\"deviceBrand\\\" ,\" +\r\n            \"a.DEVICE_MODEL as \\\"deviceModel\\\" ,\" +\r\n            \"case a.STATUS when 0 then '停运' when 1 then '投运' when 2 then '改造中' when 3 then '临时移交' when 4 then '预入库' when 5 then '调试中' when 6 then '试运行' else '未知状态' end as \\\"status\\\" ,\" +\r\n            \"a.DEVICE_PHOTO as \\\"devicePhoto\\\" ,\" +\r\n            \"a.CREATE_TIME as \\\"createTime\\\" ,\" +\r\n            \"a.UPDATE_TIME as \\\"updateTime\\\" ,\" +\r\n            \"c.NAME as \\\"createUserName\\\",\" +\r\n            \"d.NAME as \\\"updateUserName\\\" from TBL_DEVICE_INFO_GANTRY a \" +\r\n            \"left join TBL_DEVICE_CATEGORY b on a.DEVICE_CATEGORY=b.ID \" +\r\n            \"left join SYS_DB.TBL_USER_INFO c on a.CREATE_USER_ID=c.JOB_NUM \" +\r\n            \"left join SYS_DB.TBL_USER_INFO d on a.CREATE_USER_ID=d.JOB_NUM \" +\r\n            \"where a.DEVICE_CATEGORY= #{deviceCategory} \" +\r\n            \"<when test='status != null'> \" +\r\n            \"and a.STATUS= #{status} \" +\r\n            \"</when>\"+\r\n            \"<when test='fieldBelong != null'> \" +\r\n            \"and a.FIELD_BELONG= #{fieldBelong} \" +\r\n            \"</when>\"+\r\n            \"<when test='deviceId != null'> \" +\r\n            \"and a.DEVICE_ID= #{deviceId} \" +\r\n            \"</when>\"+\r\n            \"<when test='userSide != null'> \" +\r\n            \"and a.USER_SIDE= #{userSide} \" +\r\n            \"</when>\"+\r\n            \"<when test='managementSide != null'> \" +\r\n            \"and a.MANAGEMENT_SIDE= #{managementSide} \" +\r\n            \"</when>\"+\r\n            \"<when test='serviceLife != null'> \" +\r\n            \"and a.SERVICE_LIFE= #{serviceLife} \" +\r\n            \"</when>\"+\r\n            \"</script>\"})\r\n    public List<Map> selectDeviceInfoGantry(@Param(\"deviceCategory\")Long deviceCategory, @Param(\"status\") Integer status, @Param(\"fieldBelong\") Long fieldBelong, @Param(\"deviceId\")String deviceId, @Param(\"userSide\") Long userSide, @Param(\"managementSide\") Long managementSide, @Param(\"serviceLife\") Integer serviceLife);\r\n\r\n    @Select(\"select * from TBL_DEVICE_INFO_GANTRY where DEVICE_ID= #{deviceId} and rownum = 1\")\r\n    TblDeviceInfoGantry checkDeviceIdUnique(@Param(\"deviceId\") String deviceId);\r\n\r\n    @Select(\"select * from TBL_DEVICE_INFO_GANTRY where DEVICE_NAME= #{deviceName} and rownum = 1\")\r\n    TblDeviceInfoGantry checkDeviceNameUnique(@Param(\"deviceName\") String deviceName);\r\n}\r\n
